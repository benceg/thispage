{
  "name": "this-page",
  "version": "1.0.1",
  "description": "The tiny, flexible, chainable switchboard library for server-rendered pages.",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel --presets es2015 ./src/index.js --out-file ./index.js",
    "build-umd": "./node_modules/.bin/babel --presets es2015 --plugins transform-es2015-modules-umd ./src/index.js --out-file ./index.js",
    "watch": "./node_modules/.bin/babel --watch --presets es2015 ./src/index.js --out-file ./index.js",
    "watch-umd": "./node_modules/.bin/babel --watch --presets es2015 --plugins transform-es2015-modules-umd ./src/index.js --out-file ./index.js",
    "preminify": "npm run build",
    "minify": "uglifyjs --compress --mangle -- index.js | gzip-size | pretty-bytes",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://benceg@github.com/benceg/thispage.git"
  },
  "keywords": [
    "router",
    "switchboard",
    "synchronous",
    "server rendered",
    "micro",
    "cms",
    "es6",
    "es2015"
  ],
  "author": "Ben Ceglowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benceg/thispage/issues"
  },
  "homepage": "https://github.com/benceg/thispage#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "gzip-size-cli": "^1.0.0",
    "pretty-bytes-cli": "^1.0.0",
    "uglify": "^0.1.5"
  }
}
